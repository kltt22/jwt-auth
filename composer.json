{
    "name": "yuan/jwt-auth",
    "description": "thinkphp jwt auth composer",
    "type": "library",
    "require": {
        "php": ">=8.0",
        "lcobucci/jwt": "^5.3",
        "topthink/framework": ">=6.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "yuan",
            "email": "6481894@qq.com"
        }
    ],
    "autoload": {
        "psr-4": {
             "Yuan\\JwtAuth\\": "src/"
        },
        "files": [
            "src/helper.php"
        ]
    },
    "extra": {
        "think": {
            "services": [
                "yuan\\jwt\\Service"
            ],
            "config": {
                "jwt": "config/config.php"
            }
        }
    }
}
